{
	"name": "clipper",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "web content extraction",
	"permissions": [
		"tabs","http://*/*","https://*/*"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "clippe content",
	},
	"background": {
		"scripts": ["myscript.js"],
		"persistent": false
	},
	"icons": {
		"16": "icon.png"
	}
}